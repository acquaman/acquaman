# ENERGY
#####################################

record( ao, "AM1611-4-I10:energy:eV:fbk")
{
    field( VAL, "0" )
    field( EGU, "eV" )
}

record( ao, "AM1611-4-I10:energy:eV")
{
    field( VAL, "0" )
    field( DRVH, "2000.0001" )
    field( DRVL, "200.0000" )
    field( EGU, "eV" )
    field( MDEL, "-1" )
}

record( mbbi, "AM1611-4-I10:energy:status" )
{
    field( VAL, "0" )
    field( ZRST, "MOVE DONE" )
    field( ZRVL, "0x00" )
    field( ONST, "MOVE ACTIVE" )
    field( ONVL, "0x01" )
    field( TWST, "AT LIMIT" )
    field( TWVL, "0x02" )
    field( THST, "FORCED STOP" )
    field( THVL, "0x03" )
    field( FRST, "ERROR" )
    field( FRVL, "0x04" )
}

record( ao, "AM1611-4-I10:energy:stop")
{
    field( VAL, "0" )
    field( MDEL, "-1" )
}



